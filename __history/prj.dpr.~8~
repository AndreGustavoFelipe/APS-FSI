program prj;

uses
  Vcl.Forms,
  ULogin in 'ULogin.pas' {FormLogin},
  UPrincipal in 'UPrincipal.pas' {FormPrincipal},
  UUsuarios in 'UUsuarios.pas' {FormUsuarios},
  UServicos in 'UServicos.pas' {FormServicos},
  UDashboard in 'UDashboard.pas' {FormRelatorio},
