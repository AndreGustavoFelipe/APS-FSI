program prj;

uses
  Vcl.Forms,
  ULogin in 'ULogin.pas' {FormLogin},
  UPrincipal in 'UPrincipal.pas' {FormPrincipal},
  UUsuarios in 'UUsuarios.pas' {FormUsuarios},
  UServicos in 'UServicos.pas' {FormServicos},
  UDashboard in 'UDashboard.pas' {FormDashboard},
  UDataModule in 'UDataModule.pas' {dm: TDataModule},
  UAgendamentos in 'UAgendamentos.pas' {FormAgendamento},
  UMensagem in 'UMensagem.pas' {FormMensagem},
  UItensAgendamento in 'UItensAgendamento.pas' {FormItensAgendamento},
  UReagendamento in 'UReagendamento.pas' {FormReagendamento},
  UAlteraStatus in 'UAlteraStatus.pas' {FormAlteraStatus};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormLogin, FormLogin);
  Application.CreateForm(Tdm, dm);
  Application.CreateForm(TFormAgendamento, FormAgendamento);
  Application.CreateForm(TFormMensagem, FormMensagem);
  Application.CreateForm(TFormItensAgendamento, FormItensAgendamento);
  Application.CreateForm(TFormReagendamento, FormReagendamento);
  Application.CreateForm(TFormAlteraStatus, FormAlteraStatus);
  Application.Run;
end.
